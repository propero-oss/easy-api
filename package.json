{
  "name": "@propero/easy-api",
  "version": "0.0.0",
  "description": "Build powerful Rest APIs with simple typescript decorators",
  "since": 2020,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "start": "node dist/index.js",
    "clean": "rimraf dist coverage",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint --ignore-path .gitignore --ext .ts .",
    "lint:fix": "eslint --ignore-path .gitignore --ext .ts . --fix"
  },
  "keywords": [
    "easy",
    "api",
    "rest",
    "typescript",
    "decorator"
  ],
  "author": "Propero Team <team@propero.dev>",
  "license": "MIT",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/npm": "^9.0.2",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@swc/core": "^1.3.31",
    "@types/express": "^4.17.16",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.1",
    "jest-preset-typescript": "^1.2.0",
    "lodash": "^4.17.21",
    "prettier": "^2.8.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.2",
    "semantic-release": "^20.1.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "test": "test",
    "example": "example",
    "lib": "src"
  },
  "dependencies": {
    "express": "^4.17.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/propero-oss/easy-api.git"
  },
  "bugs": {
    "url": "https://github.com/propero-oss/easy-api/issues"
  },
  "homepage": "https://github.com/propero-oss/easy-api#readme",
  "release": {
    "branches": ["main"]
  }
